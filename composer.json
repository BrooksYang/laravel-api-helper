{
    "name": "brooksyang/laravel-api-helper",
    "description": "Api doc generator for laravel 5",
    "license": "MIT",
    "authors": [
        {
            "name": "BrooksYang",
            "email": "brooksyang@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.5",
        "predis/predis": ">=1.1",
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "BrooksYang\\LaravelApiHelper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BrooksYang\\LaravelApiHelper\\ApiHelperServiceProvider"
            ],
            "aliases": {
                "Doc": "BrooksYang\\LaravelApiHelper\\Facades\\Doc"
            }
        }
    }
}
